<?xml version="1.0" encoding="utf-8"?>
<odoo>

         <record id="view_hr_leave_report_tree_changes" model="ir.ui.view">
            <field name="name">hr_leave.report.changes</field>
            <field name="model">hr.leave.report</field>
            <field name="inherit_id" ref="hr_holidays.hr_leave_report_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="is_annual" invisible="1"/>
                    <field name="valid" invisible="1"/>
                </xpath>
            </field>
        </record>
</odoo>
